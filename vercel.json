{
  "version": 2,
  "buildCommand": "cd merchant-dashboard && npm install && npm run build",
  "outputDirectory": "merchant-dashboard/.next",
  "installCommand": "npm install --prefix ./merchant-dashboard",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/dashboard(.*)",
      "destination": "/merchant-dashboard/$1"
    },
    {
      "source": "/admin(.*)",
      "destination": "/merchant-dashboard/$1"
    }
  ]
}
